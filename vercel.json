{
  "rewrites": [
    {
      "source": "/.well-known/widget.json",
      "destination": "/public/.well-known/widget.json"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/widget.json",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}